;; pathnet millenium upgrade added two other code values with display key CHW
select into "nl:"
     cv.code_value
from code_value cv
plan cv
where cv.code_set=220
and cv.display_key="CHW"
and cv.cdf_meaning="FACILITY"
and cv.active_ind=1
